    "homepage": "https://github.com/archer-developer/GarantFilePreviewGenerator",
    "authors": [
        {
            "name": "Alexander Samusevich",
            "email": "archer.developer@gmail.com"
        }
    ],
    "require": {
        "php": "^5.3.9|^7.0",
        "ext-imagick": "*",
        "react/http": "^0.5@dev",
        "liip/imagine-bundle": "dev-master",
        "guzzlehttp/guzzle": "dev-master",
        "symfony/framework-bundle": "~2.3|~3.0"
    },
    "autoload": {
        "psr-4": { "Garant\\FilePreviewGeneratorBundle\\": "" }
